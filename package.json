  "description": "Hebcal",
  "main": "./dist/index.js",
  "dependencies": {
    "gematriya": "^2.0.0",
    "suncalc": "^1.8.0",
    "ttag": "^1.7.22"
  },
  "scripts": {
    "build": "babel src --presets babel-preset-env --out-dir dist",
    "prepublish": "npm run build",
    "po2json": "for infile in ../hebcal/po/*.po; outfile=$(basename -s .po $infile); echo $outfile; npx ttag po2json $infile > src/$outfile.json; done && exit 0",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
