{
  "name": "hebcal-es6",
  "version": "1.0.0",
  "author": "Michael J. Radwin (https://github.com/mjradwin)",
  "contributors": [
    "Eyal Schachter (https://github.com/Scimonster)",
    "Danny Sadinoff (https://github.com/dsadinoff)"
  ],
  "description": "Hebcal",
  "main": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/mjradwin/hebcal-es6.git"
  },
  "bugs": {
    "url": "https://github.com/mjradwin/hebcal-es6/issues"
  },
  "dependencies": {
    "gematriya": "^2.0.0",
    "suncalc": "^1.8.0",
    "ttag": "^1.7.22"
  },
  "scripts": {
    "build:babel": "babel src --presets babel-preset-env --out-dir dist",
    "build:cp-json": "mkdir -p ./dist && cp ./src/*.json ./dist",
    "build": "npm run po2json && npm run build:babel && npm run build:cp-json",
    "prepublish": "npm run build",
    "po2json": "mkdir -p dist && for infile in ./po/*.po; do outfile=`basename -s .po $infile`; echo $outfile; npx ttag po2json $infile > dist/$outfile.po.json; done && exit 0",
    "readme": "jsdoc2md src/*.js",
    "typedef": "jsdoc -t node_modules/tsd-jsdoc/dist -r ./src/",
    "test": "ava"
  },
  "license": "GPL-2.0",
  "ava": {
    "files": [
      "dist/**/*.spec.js"
    ],
    "require": [
      "babel-register"
    ],
    "inherit": true,
    "verbose": true
  },
  "devDependencies": {
    "ava": "^3.8.2",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "jsdoc": "^3.6.4",
    "jsdoc-to-markdown": "^5.0.3",
    "tsd-jsdoc": "^2.5.0",
    "ttag-cli": "^1.8.2"
  }
}
