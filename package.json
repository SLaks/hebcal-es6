{
  "name": "hebcal-es6",
  "version": "1.0.0",
  "author": "Michael J. Radwin (https://github.com/mjradwin)",
  "contributors": [
    "Eyal Schachter (https://github.com/Scimonster)",
    "Danny Sadinoff (https://github.com/dsadinoff)"
  ],
  "description": "Hebcal",
  "main": "./dist/hebcal.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/mjradwin/hebcal-es6.git"
  },
  "bugs": {
    "url": "https://github.com/mjradwin/hebcal-es6/issues"
  },
  "dependencies": {
    "core-js": "^2.6.11",
    "gematriya": "^2.0.0",
    "md5": "^2.2.1",
    "source-map-support": "^0.5.19",
    "suncalc": "^1.8.0",
    "ttag": "^1.7.22"
  },
  "scripts": {
    "build:babel": "babel src --out-dir dist --source-maps",
    "build:cp-json": "mkdir -p ./dist && cp ./src/*.json ./dist",
    "build": "npm run po2json && npm run build:babel && npm run build:cp-json",
    "prepublish": "npm run build",
    "po2json": "mkdir -p dist && for infile in ./po/*.po; do outfile=`basename -s .po $infile`; echo $outfile; npx ttag po2json $infile --format compact > dist/$outfile.po.json; done && exit 0",
    "readme": "npx jsdoc2md dist/*.js",
    "typedef": "jsdoc -t node_modules/tsd-jsdoc/dist -r ./src/",
    "test": "ava"
  },
  "license": "GPL-2.0",
  "ava": {
    "files": [
      "dist/**/*.spec.js"
    ],
    "inherit": true,
    "verbose": true
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "ava": "^3.8.2",
    "jsdoc": "^3.6.4",
    "jsdoc-to-markdown": "^5.0.3",
    "tsd-jsdoc": "^2.5.0",
    "ttag-cli": "^1.8.2"
  }
}
